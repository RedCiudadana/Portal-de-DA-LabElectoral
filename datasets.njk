<script>
	window.onload=function() {
		if("{{datos.Enlace1}}"==""||"{{datos.Enlace1}}"==" "){
			var x = document.getElementById("enlacePdf");
			if (x.style.display === "none") {
				x.style.display = "block";
			} else {
